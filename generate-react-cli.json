{
  "usesTypeScript": false,
  "usesStyledComponents": true,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/components",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true
    },
    "page": {
      "path": "src/pages",
      "customTemplates": {
        "component": "templates/page/TemplateName.jsx",
        "style": "templates/page/TemplateName.module.scss"
      },
      "withLazy": true,
      "withStory": false,
      "withStyle": true,
      "withTest": true
    }
  }
}